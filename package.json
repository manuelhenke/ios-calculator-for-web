{
  "name": "ios-calculator-for-web",
  "version": "1.2.5",
  "description": "This is a calculator for the web following the design of the iOS-Calculator.",
  "keywords": [
    "calculator",
    "javascript",
    "webcomponent",
    "custom-element"
  ],
  "homepage": "https://henkebyte.com/ios-calculator",
  "bugs": {
    "url": "https://github.com/manuelhenke/ios-calculator-for-web/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuelhenke/ios-calculator-for-web.git"
  },
  "license": "MIT",
  "author": "Manuel Henke <manuel.henke@yahoo.de> (https://github.com/manuelhenke)",
  "type": "module",
  "main": "dist/ios-calculator.js",
  "module": "dist/ios-calculator.js",
  "scripts": {
    "analyze": "cem analyze",
    "build": "webpack -c webpack.cjs",
    "format": "eslint --ext .js,.html,.scss . --fix --ignore-path .gitignore && prettier \"**/*.js\" --write --ignore-path .gitignore",
    "lint": "eslint --ext .js,.html,.scss . --ignore-path .gitignore && prettier \"**/*.js\" --check --ignore-path .gitignore",
    "test": "web-test-runner --coverage",
    "test:watch": "web-test-runner --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env"
      ]
    ]
  },
  "browserslist": [
    "last 2 years",
    "> 1%",
    "not dead"
  ],
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "@open-wc",
      "prettier"
    ]
  },
  "dependencies": {
    "mathjs": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@custom-elements-manifest/analyzer": "^0.5.7",
    "@open-wc/eslint-config": "^7.0.0",
    "@open-wc/testing": "^3.0.4",
    "@web/test-runner": "0.13.27",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-lit-a11y": "^2.2.0",
    "eslint-plugin-wc": "^1.3.2",
    "html-loader": "^3.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "postcss": "^8.4.4",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "sass": "^1.49.8",
    "sass-loader": "^12.6.0",
    "stylelint": "^14.5.3",
    "stylelint-config-standard": "^25.0.0",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "customElements": "custom-elements.json",
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "stylelint": {}
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  }
}
