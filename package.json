  "browser": "dist/ios-calculator.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "analyze": "cem analyze",
    "build": "webpack && yarn analyze --exclude dist",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
